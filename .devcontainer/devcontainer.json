{
  "workspaceFolder": "/j-pandas-dataset",
  "workspaceMount": "source=${localWorkspaceFolder},target=${containerWorkspaceFolder},type=bind,consistency=cached",
  "postCreateCommand": "poetry install --no-interaction --no-ansi && poetry run pre-commit install",
  "image": "ghcr.io/openjksoftware/python-devcontainer:3.11",
  "remoteUser": "ContainerUser",
  "mounts": [
    "source=vscode_cache,target=/home/vscode/.vscode-server,type=volume"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "visualstudioexptteam.vscodeintellicode",
        "ms-python.vscode-pylance",
        "christian-kohler.path-intellisense",
        "njpwerner.autodocstring",
        "eamodio.gitlens",
        "GitLab.gitlab-workflow",
        "esbenp.prettier-vscode",
        "naumovs.color-highlight",
        "cstrap.flask-snippets",
        "mechatroner.rainbow-csv",
        "bungcip.better-toml"
      ]
    }
  }
}
